# Logging Configuration
# Production ortamı için minimal logging

# Database logging - sadece error'ları logla
PRISMA_LOG_LEVEL=error

# CSRF debug - sadece gerektiğinde aç
CSRF_DEBUG=false

# Performance monitoring - production'da kapat
ENABLE_PERFORMANCE_MONITORING=false

# API request logging - production'da kapat  
ENABLE_REQUEST_LOGGING=false

# Development için bu ayarları kullan:
# PRISMA_LOG_LEVEL=warn
# CSRF_DEBUG=true
# ENABLE_PERFORMANCE_MONITORING=true
# ENABLE_REQUEST_LOGGING=true
